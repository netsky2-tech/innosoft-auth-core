{
  "name": "innosoft/auth-core",
  "description": "Core Authentication, Authorization (RBAC) and Security Package for InnoSoft Ecosystem",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Octavio Morales",
      "email": "octaviomr9810@gmail.com",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php": "^8.3",
    "bacon/bacon-qr-code": "^3.0",
    "illuminate/auth": "^11.0|^12.0",
    "illuminate/contracts": "^11.0|^12.0",
    "illuminate/support": "^11.0|^12.0",
    "laravel/sanctum": "^4.0",
    "pragmarx/google2fa-laravel": "^2.3",
    "ramsey/uuid": "^4.9",
    "spatie/laravel-permission": "^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "pestphp/pest": "^3.0",
    "pestphp/pest-plugin-laravel": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "InnoSoft\\AuthCore\\": "src/",
      "InnoSoft\\AuthCore\\Database\\Factories\\": "database/factories/",
      "InnoSoft\\AuthCore\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "InnoSoft\\AuthCore\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "InnoSoft\\AuthCore\\AuthCoreServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
